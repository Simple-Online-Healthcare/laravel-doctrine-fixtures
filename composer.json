{
  "name": "simple-online-healthcare/laravel-doctrine-fixtures",
  "description": "Adds Doctrine Fixtures support within Laravel",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Josh Murray",
      "email": "josh.murray@simpleonlinehealthcare.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SimpleOnlineHealthcare\\Doctrine\\": "src"
    }
  },
  "require": {
    "laravel-doctrine/orm": "^1.8|^2.0",
    "laravel/framework": "^9.19|^10.10",
    "doctrine/data-fixtures": "^1.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SimpleOnlineHealthcare\\Doctrine\\LaravelDoctrineFixturesServiceProvider"
      ]
    }
  }
}
